              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="SetCurrentPresetResponse">
        <xs:complexType>
          <xs:sequence/>
        </xs:complexType>
      </xs:element>
